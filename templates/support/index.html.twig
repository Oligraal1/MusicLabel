{% extends 'base.html.twig' %}

{% block title %}Support index{% endblock %}

{% block body %}
    <h2>Support index</h2>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type_support</th>
                <th>Prix</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for support in supports %}
            <tr>
                <td>{{ support.id }}</td>
                <td>{{ support.typeSupport }}</td>
                <td>{{ support.prix }}</td>
                <td>
                    <a href="{{ path('support_show', {'id': support.id}) }}">show</a>
                    <a href="{{ path('support_edit', {'id': support.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('support_new') }}">Create new</a>
{% endblock %}
